     <text:p text:style-name="P5"><office:annotation office:name="__Annotation__4623_3395360522">
       <dc:creator>Unknown Author</dc:creator>
       <dc:date>2018-09-17T11:39:52.809424572</dc:date>
       <text:p text:style-name="P11"><text:span text:style-name="T24">longest common subsequence for c_str</text:span></text:p>
      </office:annotation>int SuggestMgr::<text:span text:style-name="T11">lcslen</text:span>(const char* s, const char* s2) {<office:annotation-end office:name="__Annotation__4623_3395360522"/></text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>int m;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>int n;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>int i;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>int j;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>char* result;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>int len = 0;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>lcs(s, s2, &amp;m, &amp;n, &amp;result);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>if (!result)</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>return 0;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>i = m;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>j = n;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>while ((i != 0) &amp;&amp; (j != 0)) {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>if (result[i * (n + 1) + j] == LCS_UPLEFT) {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="6"/>len++;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="6"/>i--;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="6"/>j--;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>} else if (result[i * (n + 1) + j] == LCS_UP) {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="6"/>i--;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>} else</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="6"/>j--;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>}</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>free(result);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>return len;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5">}</text:p>
    </text:list-item>
    <text:list-item>
