    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>int l2 = strlen(s2);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>if (l1 &lt;= l2 &amp;&amp; s2[l1 - 1] == s2[l2 - 1])</text:p>
