    <text:list-item>
     <text:p text:style-name="P7"><text:s text:c="2"/>if (utf8) {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P7"><text:s text:c="4"/>m = u8_u16(su, s);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P7"><text:s text:c="4"/>n = u8_u16(su2, s2);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P7"><text:s text:c="2"/>} else {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>m = strlen(s);</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>n = strlen(s2);</text:p>
