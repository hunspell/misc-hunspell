     <text:p text:style-name="P5"><text:s text:c="2"/>}</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>c = (char*)malloc((m + 1) * (n + 1));</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>b = (char*)malloc((m + 1) * (n + 1));</text:p>
