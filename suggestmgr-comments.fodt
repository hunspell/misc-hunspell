       <text:p text:style-name="P10"><text:span text:style-name="T24">validation of useful allocation</text:span></text:p>
      </office:annotation>if (!c || !b) {</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="4"/>if (c)</text:p>
