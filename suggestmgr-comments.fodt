    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>}</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5"><text:s text:c="2"/>return 0;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P5">}</text:p>
    </text:list-item>
